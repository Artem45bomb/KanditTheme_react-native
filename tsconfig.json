{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@kandit/*":[
        "./kandit-theme/*"
      ],
      "@li/*": [
        "./kandit-theme/*"
      ],
      "@src/*":[
        "./kandit-theme/src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
